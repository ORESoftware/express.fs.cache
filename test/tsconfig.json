{
  "compilerOptions": {
    "allowJs": false,
    "pretty": true,
    "rootDir": ".",
    "skipLibCheck": true,
    "declaration": false,
    "baseUrl": ".",
    "target": "es2019",
    "module": "commonjs",
    "noImplicitAny": true,
    "removeComments": true,
    "allowUnreachableCode": true,
    "lib": [
      "es2017",
      "es2018",
      "es2019"
    ]
  },
  "compileOnSave": false
}

